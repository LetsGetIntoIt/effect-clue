{
	"compilerOptions": {
		"target": "ES2020",
		"module": "ESNext",
		"moduleResolution": "bundler",
		"noEmit": true,
		"allowJs": false,
		"checkJs": true,
		"jsx": "react-jsx",
		"jsxImportSource": "preact",
		"strict": true,
		"plugins": [
			{ "name": "@effect/language-service" }
		],
	},
	"include": ["node_modules/vite/client.d.ts", "**/*"]
}
