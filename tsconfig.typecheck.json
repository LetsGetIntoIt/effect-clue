{
    "extends": "./tsconfig.json",
    "compilerOptions": {
      "resolveJsonModule": true,
      "strict": true,
      "noUncheckedIndexedAccess": true,
      "sourceMap": false,
      "noEmit": true
    },
    "files": ["package.json", "tsconfig.json", "tsconfig.typecheck.json", "tsconfig.build.json", ".vscode/extensions.json", ".vscode/settings.json"],
    "include": ["*.*", "**/*.*"]
}
